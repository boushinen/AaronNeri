@model IEnumerable<BOOKRESERVATION.Models.Account>

@Html.ActionLink("Home", "Homepage", "Home") 

<br />


@Html.ActionLink("Logout", "Logout")
<br />


<form method="get" action="@Url.Action("Index", "Admin")">
    <label>Search Data</label>
    <input type="text" name="search" />
    <button type="submit">Search</button>
</form>


@Html.ActionLink("ADD ACCOUNTS", "add")

<br/>                                       
    
@foreach (var item in Model)
{
     
    @Html.DisplayFor(asdf => item.Id); 
    <text>&nbsp;</text>
    @Html.DisplayFor(asdf => item.Name);
    <text>&nbsp;</text>
    @Html.DisplayFor(asdf => item.Age);
    <text>&nbsp;</text>
    @Html.DisplayFor(asdf => item.Password);
    <text>&nbsp;</text>
    @Html.DisplayFor(asdf => item.Username);
    <text>&nbsp;</text>
    @Html.DisplayFor(asdf => item.Address);
    <text>&nbsp;</text>

    @Html.ActionLink("Edit", "Edit", new { Id = item.Id })
    <text>&nbsp;</text>

    @Html.ActionLink("Delete", "Delete", new { Id = item.Id })
    <br />
}

<br/>                                       
    
